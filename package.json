{
  "name": "mern_supermarket_list",
  "version": "1.0.0",
  "description": "shopping list built with Mern stack ",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js --proxy-config proxy.config.json",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Youmna",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "build": "^0.1.4",
    "concurrently": "^5.1.0",
    "config": "^3.3.0",
    "dispatch": "^1.0.2",
    "express": "^4.17.1",
    "git": "^0.1.5",
    "init": "^0.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.5",
    "nodejs": "0.0.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
